<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.GUI.Zusammenfassung">
  <children>
    <BorderPane layoutX="0.0" layoutY="0.0" prefHeight="600.0" prefWidth="400.0">
      <bottom>
        <Button fx:id="zurück" mnemonicParsing="false" onAction="#handleButtonAction" text="Zurück zur Quiz Auswahl" wrapText="true" BorderPane.alignment="CENTER">
          <BorderPane.margin>
            <Insets bottom="75.0" />
          </BorderPane.margin>
        </Button>
      </bottom>
      <center>
        <GridPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="300.0">
          <children>
            <Label text="Richtig beantwortet" wrapText="true" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER">
              <GridPane.margin>
                <Insets top="100.0" fx:id="x1" />
              </GridPane.margin>
            </Label>
            <Label fx:id="right" text="Label" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.margin="$x1" GridPane.rowIndex="0" GridPane.valignment="CENTER" />
            <Label text="Falsch beantwortet" wrapText="true" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
              <GridPane.margin>
                <Insets bottom="100.0" fx:id="x2" />
              </GridPane.margin>
            </Label>
            <Label fx:id="wrong" text="Label" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.margin="$x2" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.0" minWidth="10.0" prefWidth="206.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="149.0" minWidth="10.0" prefWidth="94.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
        </GridPane>
      </center>
      <top>
        <Label alignment="CENTER" contentDisplay="CENTER" text="Ergebnis" BorderPane.alignment="CENTER">
          <BorderPane.margin>
            <Insets top="50.0" />
          </BorderPane.margin>
        </Label>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
